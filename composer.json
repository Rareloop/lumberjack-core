{
    "name": "rareloop/lumberjack-core",
    "description": "A powerful MVC framework for the modern WordPress developer. Write better, more expressive and easier to maintain code",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "php-di/php-di": "^6.0.0",
        "rareloop/router": "^4.4.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "blast/facades": "^1.0",
        "timber/timber": "^1.7",
        "monolog/monolog": "^1.23",
        "http-interop/response-sender": "^1.0",
        "symfony/debug": "^4.1",
        "tightenco/collect": "^5.6.0",
        "statamic/stringy": "~3.1.0",
        "laminas/laminas-diactoros": "^2.4",
        "rareloop/psr7-server-request-extension": "^2.1.0",
        "mmeyer2k/dcrypt": "^8.0.1",
        "spatie/macroable": "^1.0",
        "mindplay/middleman": "^3.0.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.3.0",
        "brain/monkey": "~2.4.0",
        "squizlabs/php_codesniffer": "^3.6.0",
        "php-mock/php-mock": "^2.0",
        "codedungeon/phpunit-result-printer": "^0.19.13",
        "codedungeon/php-cli-colors": "^1.10.7",
        "mikey179/vfsstream": "^1.6.8",
        "antecedent/patchwork": "^2.1.8"
    },
    "autoload": {
        "psr-4": {
            "Rareloop\\Lumberjack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rareloop\\Lumberjack\\Test\\": "tests"
        }
    }
}
